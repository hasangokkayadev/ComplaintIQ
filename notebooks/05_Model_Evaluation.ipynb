    "## 6. Model Stability ve Cross-Validation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cross-validation stability\n",
    "from sklearn.model_selection import StratifiedKFold\n",
    "\n",
    "cv = StratifiedKFold(n_splits=5, shuffle=True, random_state=42)\n",
    "\n",
    "cv_scores = cross_val_score(final_model, X_train, y_train, cv=cv, scoring='f1_weighted')\n",
    "cv_accuracy = cross_val_score(final_model, X_train, y_train, cv=cv, scoring='accuracy')\n",
    "\n",
    "print(\"=== CROSS-VALIDATION STABILITY ===\")\n",
    "print(f\"F1-Score CV SkorlarÄ±: {cv_scores}\")\n",
    "print(f\"F1-Score Ortalama: {cv_scores.mean():.4f} (+/- {cv_scores.std() * 2:.4f})\")\n",
    "print(f\"Accuracy CV SkorlarÄ±: {cv_accuracy}\")\n",
    "print(f\"Accuracy Ortalama: {cv_accuracy.mean():.4f} (+/- {cv_accuracy.std() * 2:.4f})\")\n",
    "\n",
    "# Model stability gÃ¶rselleÅŸtirmesi\n",
    "plt.figure(figsize=(12, 5))\n",
    "\n",
    "plt.subplot(1, 2, 1)\n",
    "plt.bar(range(1, 6), cv_scores, color='lightblue', alpha=0.7)\n",
    "plt.axhline(y=cv_scores.mean(), color='red', linestyle='--', label='Ortalama')\n",
    "plt.title('F1-Score Cross-Validation', fontweight='bold')\n",
    "plt.xlabel('Fold')\n",
    "plt.ylabel('F1-Score')\n",
    "plt.legend()\n",
    "\n",
    "plt.subplot(1, 2, 2)\n",
    "plt.bar(range(1, 6), cv_accuracy, color='lightcoral', alpha=0.7)\n",
    "plt.axhline(y=cv_accuracy.mean(), color='red', linestyle='--', label='Ortalama')\n",
    "plt.title('Accuracy Cross-Validation', fontweight='bold')\n",
    "plt.xlabel('Fold')\n",
    "plt.ylabel('Accuracy')\n",
    "plt.legend()\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
