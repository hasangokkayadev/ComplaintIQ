  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Business gereksinimlerini kontrol et\n",
    "business_requirements = {\n",
    "    'min_accuracy': 0.75,  # Minimum %75 doÄŸruluk\n",
    "    'min_f1_weighted': 0.70,  # Minimum %70 F1-score\n",
    "    'max_prediction_time': 0.1,  # Maksimum 0.1 saniye\n",
    "    'min_confidence_threshold': 0.6  # Minimum %60 gÃ¼ven\n",
    "}\n",
    "\n",
    "# DoÄŸruluk kontrolÃ¼\n",
    "accuracy_check = accuracy >= business_requirements['min_accuracy']\n",
    "f1_check = f1_weighted >= business_requirements['min_f1_weighted']\n",
    "\n",
    "# Tahmin sÃ¼resi Ã¶lÃ§Ã¼mÃ¼\n",
    "start_time = time.time()\n",
    "_ = final_model.predict(X_test[:100])  # 100 Ã¶rnek\n",
    "prediction_time = (time.time() - start_time) / 100\n",
    "speed_check = prediction_time <= business_requirements['max_prediction_time']\n",
    "\n",
    "# GÃ¼ven skorlarÄ± analizi\n",
    "max_probabilities = y_pred_proba.max(axis=1)\n",
    "avg_confidence = max_probabilities.mean()\n",
    "low_confidence_ratio = (max_probabilities < business_requirements['min_confidence_threshold']).mean()\n",
    "confidence_check = avg_confidence >= business_requirements['min_confidence_threshold']\n",
    "\n",
    "print(\"=== BUSINESS REQUIREMENTS VALIDATION ===\")\n",
    "print(f\"Minimum DoÄŸruluk ({business_requirements['min_accuracy']:.0%}): {accuracy:.4f} - {'âœ… PASS' if accuracy_check else 'âŒ FAIL'}\")\n",
    "print(f\"Minimum F1-Score ({business_requirements['min_f1_weighted']:.0%}): {f1_weighted:.4f} - {'âœ… PASS' if f1_check else 'âŒ FAIL'}\")\n",
    "print(f\"Tahmin SÃ¼resi (<{business_requirements['max_prediction_time']}s): {prediction_time:.4f}s - {'âœ… PASS' if speed_check else 'âŒ FAIL'}\")\n",
    "print(f\"Ortalama GÃ¼ven Skoru ({business_requirements['min_confidence_threshold']:.0%}): {avg_confidence:.4f} - {'âœ… PASS' if confidence_check else 'âŒ FAIL'}\")\n",
    "print(f\"DÃ¼ÅŸÃ¼k GÃ¼ven Skorlu Tahminler: {low_confidence_ratio:.1%}\")\n",
    "\n",
    "# Genel sonuÃ§\n",
    "all_requirements_met = all([accuracy_check, f1_check, speed_check, confidence_check])\n",
    "print(f\"\\n{'ğŸ‰ TÃœM BUSINESS GEREKSINIMLERI KARÅILANIYOR!' if all_requirements_met else 'âš ï¸ BAZI GEREKSINIMLER KARÅILANMIYOR'}\")"
