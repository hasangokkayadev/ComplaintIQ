   "source": [
    "# Feature importance (coefficients)\n",
    "feature_names = tfidf.get_feature_names_out()\n",
    "coefficients = final_model.coef_\n",
    "\n",
    "# Her kategori iÃ§in en Ã¶nemli Ã¶zellikler\n",
    "top_features_per_category = {}\n",
    "\n",
    "for i, category in enumerate(final_model.classes_):\n",
    "    coef = coefficients[i]\n",
    "    top_positive_idx = np.argsort(coef)[-10:][::-1]\n",
    "    top_negative_idx = np.argsort(coef)[:10]\n",
    "    \n",
    "    top_features_per_category[category] = {\n",
    "        'positive': [(feature_names[idx], coef[idx]) for idx in top_positive_idx],\n",
    "        'negative': [(feature_names[idx], coef[idx]) for idx in top_negative_idx]\n",
    "    }\n",
    "\n",
    "print(\"=== HER KATEGORI Ä°Ã‡Ä°N EN Ã–NEMLI 5 POZITIF Ã–ZELLÄ°K ===\")\n",
    "for category, features in top_features_per_category.items():\n",
    "    print(f\"\\n{category}:\")\n",
    "    for feature, coef in features['positive'][:5]:\n",
    "        print(f\"  {feature}: {coef:.4f}\")"
   ]
